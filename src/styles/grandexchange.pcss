@import url("https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700;900&display=swap");

@import "./partials/navbar.pcss";
@import "./partials/items.pcss";

body {
  @apply flex justify-start items-start p-0;
}

h2 {
  @apply m-0;
}

main {
  @apply p-8;
  margin-left: 200px;

  h2 {
    @apply font-mont font-bold;
    font-size: 1.5rem;
  }
}
